{
  "name": "discord-bypasses",
  "version": "1.4.8",
  "description": "A collection of bypasses and utilities. Take a look in the plugin's settings for the features.",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "replugged build plugin",
    "watch": "replugged build plugin --watch",
    "bundle": "replugged bundle plugin",
    "release": "replugged release",
